{
  "name": "kield-01/youtube",
  "description": "Laravel PHP Facade/Wrapper for the Youtube Data API v3",
  "keywords": ["youtube", "api", "video", "laravel", "alaouy"],
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Kozin",
      "email": "romanko95.rk@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1"
  },
  "autoload": {
    "psr-4": {
      "KielD01\\Youtube\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KielD01\\Youtube\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "KielD01\\Youtube\\YoutubeServiceProvider"
      ],
      "aliases": {
        "Youtube": "KielD01\\Youtube\\Facades\\Youtube"
      }
    }
  }
}
